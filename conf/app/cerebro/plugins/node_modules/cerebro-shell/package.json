{
  "name": "cerebro-shell",
  "version": "0.1.4",
  "description": "Cerebro plugin to execute any shell command from Cerebro search line",
  "license": "MIT",
  "repository": "KELiON/cerebro-shell",
  "author": {
    "name": "Alexandr Subbotin",
